            "large": "img/gtom-logo.svg"
        },
        "links": [
            {
                "name": "GitHub",
                "url": "https://github.com/gtom"
            },
            {
                "name": "MIT License",
                "url": "https://github.com/gtom/blob/main/LICENSE"
            }
        ],
        "screenshots": [
          { "name": "Query", "path": "img/query.png" },
          { "name": "Annotation", "path": "img/annotation.png" },
          { "name": "Variable", "path": "img/variable.png" }
        ],
        "version": "%VERSION%",
        "updated": "%TODAY%"
    },
    "dependencies": {
        "grafanaDependency": ">=9.0.0",
        "plugins": []
    }
