{
    "$schema": "https://raw.githubusercontent.com/grafana/grafana/master/docs/sources/developers/plugins/plugin.schema.json",
    "name": "Gtom",
    "id": "gtom",
    "type": "datasource",
    "annotations": true,
    "alerting": true,
    "backend": true,
    "executable": "gpx_gtom",
    "metrics": true,
    "logs": true,
    "info": {
